[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	ls-gone = ! "git fetch -p && git for-each-ref --format '%(refname:short) %(upstream:track)' | awk '$3 == \"[gone]\" {print $1}'"
	gone = ! "git fetch -p && git for-each-ref --format '%(refname:short) %(upstream:track)' | awk '$3 == \"[gone]\" {print $1}' | xargs git branch -D"
